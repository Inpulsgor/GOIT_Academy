<h1>{{name}}</h1>
<div class="wrap">
    <div class="left">
        <h3>Capital: <span>{{capital}}</span></h3>
        <h3>Population: <span>{{population}}</span></h3>
        <h3>Languages:</h3>
        <ul class="languagesList">
            {{#each languages}}
            <li class="countryListItem">{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="right">
        <img src="{{flag}}" alt="{{nativeName}}" width="500" height="400">
    </div>
</div>